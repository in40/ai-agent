# Qdrant Cleanup Configuration Example
#
# Copy this file to .env and update the values for your environment
#

# Qdrant connection settings
export QDRANT_HOST=localhost
export QDRANT_PORT=6333
export QDRANT_API_KEY=your_api_key_here  # Leave empty if no authentication required
export QDRANT_HTTPS=false  # Set to true if using HTTPS; set to false if using API key with HTTP
export QDRANT_VERIFY_SSL=true  # Set to false to disable SSL certificate verification

# NOTE: If using API key authentication with HTTP (not HTTPS), you MUST set QDRANT_HTTPS=false
# This is because the qdrant-client library may force SSL negotiation when an API key is provided

# Alternative: Use full URL (especially for cloud instances)
# export QDRANT_URL=https://your-cluster-id.eu-central.aws.cloud.qdrant.io:6333
# export QDRANT_API_KEY=your_cloud_api_key_here

# Optional: URL prefix if Qdrant is behind a proxy
# export QDRANT_PREFIX=/qdrant